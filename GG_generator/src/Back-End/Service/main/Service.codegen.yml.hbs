overwrite: true
schema: "http://localhost:{{ ServicePort }}/graphql"
documents: null
generates:
  src/graphql/generated/graphql.ts:
    plugins:
      - "typescript"
      - "typescript-resolvers"
    config:
      contextType: src/config/prisma/IPrismaContext#IPrismaContext
      useIndexSignature: true
      federation: true
